{
    "name": "aventailltd/dump",
    "type": "library",
    "description": "Dump mysql database with custom rules",
    "keywords": ["DB", "MySQL", "Dump"],
    "authors": [
        {
            "name": "László Szalma",
            "email": "laszlo.szalma@aventail.hu"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.0"
        }
    },
    "license": "MIT",
    "require": {
        "ext-pdo": "*",
        "ifsnop/mysqldump-php": "dev-PHP_8.2",
        "fakerphp/faker": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "AventailLtd\\Dump\\": "src/",
            "": "include/"
        },
        "classmap": ["include/"]
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/CallMeJon90/mysqldump-php"
      }
    ]
}
